{
  "manifestVersion": 1,
  "id": "vsts-tools-build-extensions",
  "name": "VSTS-Tools Build Extensions",
  "version": "1.3.0",
  "public": true,
  "publisher": "moonspace-labs-llc",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Various build tasks to help extend diagnostics, retention setting, etc.",
  "categories": [
    "Build and release"
  ],
  "tags": [
    "diagnostics",
    "list files",
    "list folders"
  ],
  "screenshots": [
    {
      "path": "screenshots/extensions.png"
    }
  ],
  "content": {
    "details": {
      "path": "overview.md"
    }
  },
  "icons": {
    "default": "extension-icon.png"
  },
  "files": [
    {
      "path": "VSTSToolsListFiles"
    },
    {
      "path": "VSTSToolsListVariables"
    },
    {
      "path": "VSTSToolsKeep"
    },
    {
      "path": "VSTSToolsListApps"
    },
    {
      "path": "VSTSToolsListSystemInfo"
    }
  ],
  "scopes": [
    "vso.build_execute"
  ],
  "contributions": [
    {
      "id": "vsts-ListFiles",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "VSTSToolsListFiles"
      }
    },
    {
      "id": "vsts-ListVariables",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "VSTSToolsListVariables"
      }
    },
    {
      "id": "vsts-Keep",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "VSTSToolsKeep"
      }
    },
    {
      "id": "vsts-ListApps",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "VSTSToolsListApps"
      }
    },
    {
      "id": "vsts-ListSystemInfo",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "VSTSToolsListSystemInfo"
      }
    }
  ],
  "links": {
    "home": {
      "uri": "https://blog.devmatter.com"
    },
    "getstarted": {
      "uri": "http://blog.devmatter.com/vsts-tools-build-extensions/"
    },
    "repository": {
      "uri": "https://github.com/jbramwell/VSTS-Tools"
    },
    "support": {
      "uri": "https://github.com/jbramwell/VSTS-Tools/issues"
    },
    "issues": {
      "uri": "https://github.com/jbramwell/VSTS-Tools/issues"
    }
  },
  "branding": {
    "color": "#1EA5CE",
    "theme": "dark"
  }
}